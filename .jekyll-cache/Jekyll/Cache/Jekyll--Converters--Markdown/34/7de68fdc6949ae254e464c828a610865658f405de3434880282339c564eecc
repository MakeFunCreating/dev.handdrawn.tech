<script src="https://player.vimeo.com/api/player.js"></script>

<script type="text/plain" data-consent="analytics">

	// Cookiehub - blocks vimeo embed until analytic cookies allowed

	var tag = document.createElement('iframe');
	tag.src = "https://player.vimeo.com/video/481630637?controls=0";
	tag.width = 640;
	tag.height = 360;
	tag.frameBorder = 0;
	tag.allow = "autoplay";
	tag.setAttribute('webkitallowfullscreen', '');
	tag.setAttribute('mozallowfullscreen', '');	
	tag.setAttribute('allowfullscreen', '');
	
	var el = document.getElementById('vimeo-player30637');
	el.appendChild(tag);
	
	// Change id to remove test pattern when cookies allowed
	
	if ( document.getElementById("testPatternBg30637").id  !== null ) {
		var videoBox30637 = document.getElementById("testPatternBg30637");
		videoBox30637.style.background = "none";
		$('#videoPlayBtn30637').fadeIn(400);
	}

	// Vimeo player controls
	const iframe30637 = $('#vimeo-player30637 iframe');
    const player30637 = new Vimeo.Player(iframe30637);

    // Events: When video starts playing
    player30637.on('play', function() {
        $('#videoPlayBtn30637').fadeOut(10);
    	$('#videoPauseBtn30637').css('cursor', 'pointer');
    });

    // Events: When video ended
    player30637.on('ended', function() {
        $('#videoPlayBtn30637').fadeIn(400);
    });

    // Events: When video paused
    player30637.on('pause', function() {
    	// Re-set hover classes
		$('#videoPlayBtn30637').hover(function(){
		  $(this).css("background-color", '#eb2a85');
		  }, function(){
		  $(this).css("background-color", '#373737');
		});
    	$('#videoPlayBtn30637').fadeIn(10);
    	$('#videoPauseBtn30637').css('cursor', 'default');
    });

    // Methods: Play video when play button clicked
    $('#videoPlayBtn30637').click(function() {
    	player30637.play();
    	$('#videoPlayBtn30637').css('background-color', '#373737');
	});

    // Methods: Pause video when pause button clicked
    $('#videoPauseBtn30637').click(function() {
    	player30637.pause();
	});

</script>






          
          

        </div>

      

      
